<%- include('../headers/header-index') %>
<body>
    <div style="width: 100%; height: 80vh; display: flex; justify-content: center; align-items: center;">
        <h1>No disponible</h1>
    </div>
    <footer style="position: fixed; bottom: 0; width: 100%; text-align: center;">
        <div class="alert alert-dismissible alert-warning">
        <button type="button" class="btn-close" data-bs-dismiss="alert"></button>
        <h4 class="alert-heading">Mensaje!</h4>
        <p class="mb-0">Esta página no está disponible, <a href="#" class="alert-link">pronto se habilitará</a>.</p>
    </footer>
    
</div>
</body>
<script>
    let close = document.querySelector(".btn-close");
    close.addEventListener("click", () => {
        close.parentElement.style.display = "none";
    })
</script>
</html>